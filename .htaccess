RewriteEngine On
RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^ %{REQUEST_SCHEME}://%1%{REQUEST_URI} [R=301,L]


RewriteRule ^aviso/$ aviso.php
RewriteRule ^a-pousada/$ index.php?pagina=pagina.php&idpagina=1
